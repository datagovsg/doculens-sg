  "dependencies": {
    "@chakra-ui/cli": "^1.7.0",
    "@chakra-ui/icons": "^1.1.1",
    "@chakra-ui/react": "^1.7.4",
    "@emotion/react": "^11.5.0",
    "@emotion/styled": "^11.3.0",
    "@opengovsg/design-system-react": "0.0.5",
    "@react-pdf-viewer/core": "^2.4.1",
